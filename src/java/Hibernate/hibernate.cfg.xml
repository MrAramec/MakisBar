<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/makisbar</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">123456</property>
    <mapping resource="Entities/Tables.hbm.xml"/>
    <mapping resource="Entities/Orders.hbm.xml"/>
    <mapping resource="Entities/Productsbyorder.hbm.xml"/>
    <mapping resource="Entities/Makis.hbm.xml"/>
    <mapping resource="Entities/Restaurants.hbm.xml"/>
    <mapping resource="Entities/Waiter.hbm.xml"/>
    <mapping resource="Entities/RelRestDrk.hbm.xml"/>
    <mapping resource="Entities/Complements.hbm.xml"/>
    <mapping resource="Entities/RelRestMak.hbm.xml"/>
    <mapping resource="Entities/Typetables.hbm.xml"/>
    <mapping resource="Entities/RelRestCmp.hbm.xml"/>
    <mapping resource="Entities/Drinks.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
